//chapter3_ex4

#include<stdio.h>

int main(void)
{
	char ascii_code;

	ascii_code = 'a';
	printf("%c : 10진수=%3d, 16진수=%3x\n",ascii_code,ascii_code,ascii_code);
	
	ascii_code = 'm';
	printf("%c : 10진수=%3d, 16진수=%3x\n",ascii_code,ascii_code,ascii_code);
	
	ascii_code = 'x';
	printf("%c : 10진수=%3d, 16진수=%3x\n",ascii_code,ascii_code,ascii_code);
	
	return 0;
}
