#include<stdio.h>
#include<limits.h>

int main(void)
{
	int a=CHAR_MAX,b=CHAR_MIN;
	
	printf("char 형의 최대값 : %d\n",a);
	printf("char 형의 최대값+1 : %d\n",a+1);
	printf("char 형의 최소값 : %d\n",b);
	printf("char 형의 최소값-1 : %d\n",b-1);
			
	return 0;
}
