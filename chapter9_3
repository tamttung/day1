//chapter9_3

#include<stdio.h>
#include<math.h>

struct point{
	int x, y
};


int main()
{
	struct point p1,p2;	
	double dist;
	
	printf("p1의 x,y좌표를 입력하세요 : ");
	scanf("%d %d",&p1.x,&p1.y);
	printf("p2의 x,y좌표를 입력하세요 : ");
	scanf("%d %d",&p2.x,&p2.y);
	
	dist = sqrt(pow(p1.x-p2.x,2)+pow(p1.y-p2.y,2));
	
	printf("두 점 사이의 거리 : %.2f\n",dist);
	
	return 0;
}

