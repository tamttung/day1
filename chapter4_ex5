//chapter4_ex5

#include<stdio.h>

int main()
{
	double a;
	
	a= 1<<5;
	printf("2^5=%.0lf\n",a);
	a= 1<<10;
	printf("2^10=%.0lf\n",a);
	a= 1<<15;
	printf("2^15=%.0lf\n",a);
	a= 1<<20;
	printf("2^20=%.0lf\n",a);
	a= 1<<25;
	printf("2^25=%.0lf\n",a);
	a= 1<<30;
	printf("2^30=%.0lf\n",a);
	
	return 0;	
}
