//chapter7_ex4

#include<stdio.h>

int main()
{
	int x[3][3]={{1,2,3},{4,5,6},{7,8,9}};
	int y[3][3]={{10,20,30},{40,50,60},{70,80,90}};
	int z[3][3];
	int i,j;
	
	for(i=0;i<3;i++){
		for(j=0;j<3;j++){
			z[i][j]=x[i][j]-y[i][j];
		}
	}
	
	for(i=0;i<3;i++){
		for(j=0;j<3;j++){
			printf("%4d",z[i][j]);
		}
		printf("\n");
	}
	
	return 0;
}
	
	
	
	
	
	
	
	
	
	
	
