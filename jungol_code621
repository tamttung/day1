//jungol_code621

#include<stdio.h>

int main()
{
	int a,b;
	int *pa=&a;
	int *pb=&b;

	scanf("%d %d",pa,pb);
	
	printf("%d + %d = %d\n",*pa,*pb,*pa + *pb);	
	printf("%d - %d = %d\n",*pa,*pb,*pa - *pb);	
	printf("%d * %d = %d\n",*pa,*pb,*pa * *pb);	
	printf("%d / %d = %d\n",*pa,*pb,*pa / *pb);	
	
	return 0;
}
